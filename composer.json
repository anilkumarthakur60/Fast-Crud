{
  "name": "anil/fast-api-crud",
  "description": "Laravel Fast API CRUD with ease controller",
  "keywords": [
    "anil",
    "laravel",
    "api",
    "crud",
    "controller",
    "fast",
    "api-crud",
    "api-crud-controller",
    "fast-api-crud"
  ],
  "type": "library",
  "require": {
    "illuminate/auth": "^8.0||^9.0||^10.0||^11.0",
    "illuminate/database": "^8.0||^9.0||^10.0||^11.0",
    "illuminate/http": "^8.0||^9.0||^10.0||^11.0",
    "illuminate/pagination": "^8.0||^9.0||^10.0||^11.0",
    "illuminate/routing": "^8.0||^9.0||^10.0||^11.0",
    "illuminate/support": "^8.0||^9.0||^10.0||^11.0",
    "spatie/laravel-permission": "6.*|5.*|4.*"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Anil\\FastApiCrud\\": "src/",
      "Anil\\FastApiCrud\\Database\\Factories\\": "database/factories"
    },
    "files": [
      "src/Helpers/fastApiCrud.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Anil\\FastApiCrud\\Tests": "tests"
    }
  },
  "authors": [
    {
      "name": "Anil Kumar Thakur",
      "email": "anilkumarthakur60@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Anil\\FastApiCrud\\Providers\\ApiCrudServiceProvider"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "laravel/pint": "^1.13",
    "orchestra/testbench": "^9.0",
    "pestphp/pest": "^2.34"
  }
}
