{
  "name": "anil/fast-api-crud",
  "description": "Laravel Fast API CRUD with ease controller",
  "keywords": [
    "anil",
    "laravel",
    "api",
    "crud",
    "controller",
    "fast",
    "api-crud",
    "api-crud-controller",
    "fast-api-crud"
  ],
  "type": "library",
  "require": {
    "illuminate/auth": "^10.0||^11.0",
    "illuminate/database": "^10.0||^11.0",
    "illuminate/http": "^10.0||^11.0",
    "illuminate/pagination": "^10.0||^11.0",
    "illuminate/routing": "^10.0||^11.0",
    "illuminate/support": "^10.0||^11.0",
    "spatie/laravel-permission": "6.*|5.*|4.*"
  },
  "require-dev": {
    "larastan/larastan": "^1.0||^2.0||^3.0",
    "laravel/pint": "^1.0",
    "orchestra/testbench": "^7.0||^8.0||^9.0",
    "pestphp/pest": "^1.0||^2.34"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Anil\\FastApiCrud\\": "src/"
    },
    "files": [
      "src/Helpers/fastApiCrud.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Anil\\FastApiCrud\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Anil Kumar Thakur",
      "email": "anilkumarthakur60@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Anil\\FastApiCrud\\Providers\\ApiCrudServiceProvider"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/pest",
    "format": "vendor/bin/pint",
    "analyse": "vendor/bin/phpstan analyse"
  }
}
